.container
  .row.valign-wrapper
    %h5
      Authors
    = link_to '(Add Author)', new_author_path#, class: "btn waves-effect waves-light"
  .row
    .col.l10.m10.s12
      %table.striped
        %thead
          %tr
            %th First Name
            %th Last Name
            %th Biography
            -# %th{:colspan => "3"}
        %tbody
          = render @authors
      %br/
    .col.l2.m2.s12
      = render "layouts/right_side_panel"